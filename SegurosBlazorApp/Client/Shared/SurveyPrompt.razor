<div class="alert alert-secondary mt-4">
    <span class="oi oi-pencil me-2" aria-hidden="true"></span>
    <strong>@Title</strong>

    <span class="text-nowrap">
        por favor visita el repositorio de GitHub
        <a target="_blank" class="font-weight-bold link-dark" href="https://github.com/elblogdelbeto/SeguroVidaBlazorApi">aquí</a>
    </span>
    y se libre de descargar o aportar lo que gustes.
</div>

@code {
    // Demonstrates how a parent component can supply parameters
    [Parameter]
    public string? Title { get; set; }
}
